[core]
  excludesfile = ~/.gitignore
[fetch]
  prune = true
[merge]
  ff = only
[push]
  default = current
[alias]
  a = add
  aa = add --all
  ap = add --patch
  ca = commit --ammend
  ci = commit -v
  co = checkout
  up = smart-pull
  d = diff
  dc = diff --cached
  p = push
  b = bisect
  l = log -w --color --graph --pretty=tformat:'%Cred%h -%C(yellow)%d%Creset %s %Cgreen(%ci) %C(bold blue)%an%Creset'
[color]
  ui = auto
[include]
  path = .gitconfig.local
